      <title xml:lang="en">Coin, Tyre, A.D. 98-A.D. 99. FIC.50.75.</title>
      <department>Greek</department>
      <typeDesc>
         <objectType xlink:href="http://nomisma.org/id/coin">Coin</objectType>
         <material xlink:href="http://nomisma.org/id/ae">Bronze</material>
         <dateRange>
            <fromDate standardDate="0098">A.D. 98</fromDate>
            <toDate standardDate="0099">A.D. 99</toDate>
         </dateRange>
         <authority/>
         <geographic>
            <geogname xlink:type="simple" xlink:role="mint" xlink:href="http://nomisma.org/id/tyre">Tyre</geogname>
